{
    "compilerOptions": {
        "target": "es2018",
        "sourceMap": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./models/**/*.ts",
        "./api/**/*.ts",
        "./**/*.ts"
    ],
    "exclude": [
        "./node_modules/*",
        "./test/*"
    ]
}
